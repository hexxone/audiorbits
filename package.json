{
	"name": "audiorbits",
	"version": "2.4.0",
	"description": "Web Wallpaper for Wallpaper Engine",
	"main": "index.js",
	"sideEffects": "false",
	"scripts": {
		"prep": "rm -rf ./dist && cp -R public dist",
		"prep-win": "del \"dist\\*.*\" /Q /S && xcopy \"public\\*.*\" \"dist\\\" /e /i /y /s",
		"dbg": "yarn prep && webpack && webpack serve",
		"dbg-win": "yarn prep-win && webpack && webpack serve",
		"prod": "yarn prep && webpack --env production && webpack serve --env production",
		"prod-win": "yarn prep-win && webpack --env production && webpack serve --env production",
		"lint": "eslint ./src/we_utils/src/cthree/ --ext js,ts,jsx,tsx --fix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hexxone/audiorbits.git"
	},
	"author": "hexxone",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/hexxone/audiorbits/issues"
	},
	"homepage": "https://github.com/hexxone/audiorbits#readme",
	"dependencies": {
