{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": "off",
        "camelcase": "off",
        "no-tabs": "off",
        "max-len": "off",
		"prefer-spread":"warn",
		"guard-for-in":"warn",
		"no-invalid-this": "warn",
		"indent": [1, "tab"],
		"require-jsdoc": "warn",
		"valid-jsdoc": "warn"
    }
}
