{
	"compilerOptions": {
		"outDir": "./dist/tsc",
		"target": "ES2020",
		"module": "ES2020",
		"moduleResolution": "Node",
		"esModuleInterop": false,
		"emitDecoratorMetadata": false,
		"sourceMap": false,
		"removeComments": true,
		"baseUrl": ".",
		"paths": {
			"three": [
				"node_modules/three/src/Three"
			]
		},
		"lib": [
			"dom",
			"ES5",
			"ES2020"
		],
	},
	"include": [
		"./src/**/*.ts"
	],
	"exclude": [
		"**/*.asc",
		"**/three.ts/**"
	]
}